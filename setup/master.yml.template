apiVersion: kubeadm.k8s.io/v1alpha1
kind: MasterConfiguration
kubernetesVersion: v1.8.6
api:
  advertiseAddress: APISERVER_IP
etcd:
  endpoints: ETCD_CLUSTER
token: TOKEN
networking:
  podSubnet: POD_CIDR
  serviceSubnet: SERVICE_CIDR
controllerManagerExtraArgs:
  master: https://APISERVER_FQDN:6443
schedulerExtraArgs:
  master: https://APISERVER_FQDN:6443
apiServerCertSANs:
- APISERVER_FQDN
