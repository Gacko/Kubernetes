apiVersion: kubeadm.k8s.io/v1beta1
kind: InitConfiguration
localAPIEndpoint:
  advertiseAddress: APISERVER_IP
bootstrapTokens:
- token: TOKEN
---
apiVersion: kubeadm.k8s.io/v1beta1
kind: ClusterConfiguration
kubernetesVersion: v1.14.4
etcd:
  external:
    endpoints: ETCD_CLUSTER
networking:
  podSubnet: POD_CIDR
  serviceSubnet: SERVICE_CIDR
apiServer:
  certSANs:
  - APISERVER_FQDN
